# FastAPI/Service settings (add other vars here as needed)

# AI integration
# If true, external AI API calls are disabled and a deterministic placeholder is returned.
DISABLE_EXTERNAL_CALLS=true

# OpenAI settings (optional). If OPENAI_API_KEY is not set or DISABLE_EXTERNAL_CALLS=true,
# the service returns a placeholder explanation.
OPENAI_API_KEY=
# Default model if not provided
OPENAI_MODEL=gpt-5-nano
